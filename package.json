    "react-native": "0.41.2",
    "react-native-linear-gradient": "2.0.0",
    "react-native-router-flux": "3.37.0",
    "react-native-svg": "5.1.5",
    "react-redux": "5.0.2",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
